{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,

    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "6.0",

    "paths": {
      "@/*": ["./*"]
    },

    "types": []
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
